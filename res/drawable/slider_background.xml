<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <!-- Shadow -->
	<item>
	    <shape>
	        
		    <!-- Gradient Color -->
		    <gradient
		        android:startColor="#CF91BF"
		        android:endColor="#914296"
		        android:type="linear"
		        android:angle="90" />
		    
		    <!-- Rounded Corners -->
			<corners
		        android:radius="8dp" />
			
		</shape>
	</item>
	
    <!-- Main Background -->
    <item
        android:top="1dp"
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp" >
	    <shape>
	        
		    <!-- Gradient Color -->
		    <gradient
		        android:startColor="#CF91BF"
		        android:endColor="#914296"
		        android:type="linear"
		        android:angle="-90" />
		    
		    <!-- Rounded Corners -->
			<corners
		        android:radius="7dp" />
			
		</shape>
	</item>
    
</layer-list>